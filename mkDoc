#make Documentation

yardoc Groundhog/*.rb Groundhog/Tools/*.rb - Readme.txt - documents/*

#modify documentation
cd doc

sed -i tmphtml 's/_index/doc_index/g' *.html
mv _index.html doc_index.html
mv file.README.html file.Readme.html
rm *tmphtml

cd ..

#create RBZ
zip -r Groundhog.rbz Groundhog Groundhog_extension.rb -x *yardoc* *.DS_Store



